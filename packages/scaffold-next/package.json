{
  "name": "@armit/scaffold-next",
  "version": "0.2.5",
  "private": true,
  "homepage": "https://github.com/armitjs/armit",
  "repository": {
    "type": "git",
    "url": "https://github.com/armitjs/armit",
    "directory": "packages/scaffold-next"
  },
  "license": "MIT",
  "author": {
    "name": "Yingchun Tian",
    "url": "https://github.com/tianyingchun"
  },
  "scripts": {
    "clean": "rimraf --no-glob ./.next ./out ./coverage ./tsconfig.tsbuildinfo",
    "build": "next build",
    "build-fast": "cross-env NEXTJS_IGNORE_TYPECHECK=1 NEXTJS_IGNORE_ESLINT=1 NEXTJS_DISABLE_SENTRY=1 NEXTJS_SENTRY_UPLOAD_DRY_RUN=1 next build",
    "bundle-analyze": "cross-env ANALYZE=true NEXTJS_IGNORE_TYPECHECK=1 NEXTJS_IGNORE_ESLINT=1 NEXTJS_SENTRY_UPLOAD_DRY_RUN=1 yarn build",
    "vercel-build": "yarn share-static-hardlink && next build",
    "dev": "next dev -p 8001",
    "dev-rc": "next dev -p 8002",
    "start": "next start -p 8001",
    "start-rc": "next start -p 8002",
    "test": "yarn test-unit",
    "test-unit": "vitest run --passWithNoTests",
    "test-unit-watch": "vitest watch --ui",
    "test-e2e": "cross-env E2E_WEBSERVER_MODE=DEV playwright test",
    "?share-static-symlink": "echo 'Use this command to link assets... from shared static folder'",
    "share-static-symlink": "rimraf ./public/shared-assets && symlink-dir ../../static/assets ./public/shared-assets",
    "?share-static-hardlink": "echo 'Use this command to link assets... from shared static folder'",
    "share-static-hardlink": "rimraf ./public/shared-assets && syncdir ../../static/assets ./public/shared-assets --copy",
    "check-dist": "es-check -v",
    "check-size": "size-limit --highlight-less",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "fix-all-files": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs,.mdx,.graphql --fix",
    "next-lint": "next lint",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs,.mdx,.graphql --cache --cache-location ../../.cache/eslint/scaffold-next.eslintcache"
  },
  "browserslist": {
    "production": [
      ">0.3%",
      "not ie 11",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@babel/core": "7.24.3",
    "@dimjs/lang": "1.5.0",
    "@dimjs/model": "1.2.3",
    "@dimjs/model-react": "1.2.3",
    "@dimjs/utils": "1.5.1",
    "@sentry/nextjs": "7.109.0",
    "@sentry/react": "7.109.0",
    "@tanstack/react-query": "5.28.9",
    "@tanstack/react-query-devtools": "5.28.10",
    "@wove/react": "1.2.23",
    "client-only": "0.0.1",
    "multiparty": "4.2.3",
    "next": "14.1.4",
    "next-connect": "1.0.0",
    "next-intl": "3.10.0",
    "picocolors": "1.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "14.1.0",
    "server-only": "0.0.1"
  },
  "devDependencies": {
    "@armit/eslint-config-bases": "workspace:^",
    "@emotion/babel-plugin": "11.11.0",
    "@emotion/react": "11.11.4",
    "@next/bundle-analyzer": "14.1.4",
    "@next/env": "14.1.4",
    "@playwright/test": "1.42.1",
    "@size-limit/file": "11.1.2",
    "@svgr/webpack": "8.1.0",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/forms": "0.5.7",
    "@tailwindcss/typography": "0.5.12",
    "@testing-library/dom": "9.3.4",
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/react": "14.2.2",
    "@testing-library/user-event": "14.5.2",
    "@types/multiparty": "0.0.36",
    "@types/node": "20.12.2",
    "@types/react": "18.2.73",
    "@types/react-dom": "18.2.23",
    "@vitejs/plugin-react": "4.2.1",
    "@vitest/ui": "1.4.0",
    "autoprefixer": "10.4.19",
    "cross-env": "7.0.3",
    "es-check": "7.1.1",
    "eslint": "8.57.0",
    "eslint-config-next": "14.1.4",
    "happy-dom": "14.3.10",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.38",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-preset-env": "9.5.3",
    "prettier": "3.2.5",
    "rimraf": "5.0.5",
    "sharp": "0.33.3",
    "size-limit": "11.1.2",
    "symlink-dir": "5.2.1",
    "sync-directory": "6.0.5",
    "tailwindcss": "3.4.3",
    "typescript": "5.4.3",
    "vite": "5.2.7",
    "vite-plugin-svgr": "4.2.0",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "1.4.0"
  }
}
