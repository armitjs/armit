{
  "name": "@armit/scaffold-next",
  "version": "0.1.4",
  "private": true,
  "homepage": "https://github.com/armitjs/armit",
  "repository": {
    "type": "git",
    "url": "https://github.com/armitjs/armit",
    "directory": "packages/scaffold-next"
  },
  "license": "MIT",
  "author": {
    "name": "Yingchun Tian",
    "url": "https://github.com/tianyingchun"
  },
  "scripts": {
    "clean": "rimraf --no-glob ./.next ./out ./coverage ./tsconfig.tsbuildinfo",
    "build": "next build",
    "build-fast": "cross-env NEXTJS_IGNORE_TYPECHECK=1 NEXTJS_IGNORE_ESLINT=1 NEXTJS_DISABLE_SENTRY=1 NEXTJS_SENTRY_UPLOAD_DRY_RUN=1 next build",
    "bundle-analyze": "cross-env ANALYZE=true NEXTJS_IGNORE_TYPECHECK=1 NEXTJS_IGNORE_ESLINT=1 NEXTJS_SENTRY_UPLOAD_DRY_RUN=1 yarn build",
    "vercel-build": "yarn share-static-hardlink && next build",
    "dev": "cross-env NODE_OPTIONS=\"--inspect --max_old_space_size=8192\" next dev -p 8001",
    "dev-rc": "cross-env NODE_OPTIONS=\"--inspect --max_old_space_size=8192\" next dev -p 8002",
    "start": "next start -p 8001",
    "start-rc": "next start -p 8002",
    "test": "yarn test-unit",
    "test-unit": "vitest run",
    "test-unit-watch": "vitest watch --ui",
    "test-e2e": "cross-env E2E_WEBSERVER_MODE=DEV playwright test",
    "?share-static-symlink": "echo 'Use this command to link assets... from shared static folder'",
    "share-static-symlink": "rimraf ./public/shared-assets && symlink-dir ../../static/assets ./public/shared-assets",
    "?share-static-hardlink": "echo 'Use this command to link assets... from shared static folder'",
    "share-static-hardlink": "rimraf ./public/shared-assets && syncdir ../../static/assets ./public/shared-assets --copy",
    "check-dist": "es-check -v",
    "check-size": "size-limit --highlight-less",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "fix-all-files": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs,.mdx,.graphql --fix",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs,.mdx,.graphql --cache --cache-location ../../.cache/eslint/scaffold-next.eslintcache"
  },
  "browserslist": {
    "production": [
      ">0.3%",
      "not ie 11",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@babel/core": "7.21.0",
    "@dimjs/lang": "1.2.33",
    "@dimjs/model": "1.1.8",
    "@dimjs/model-react": "1.1.8",
    "@dimjs/utils": "1.2.33",
    "@sentry/nextjs": "7.39.0",
    "@sentry/react": "7.39.0",
    "@tanstack/react-query": "4.24.10",
    "@tanstack/react-query-devtools": "4.24.12",
    "@wove/react": "1.2.22",
    "client-only": "0.0.1",
    "multiparty": "4.2.3",
    "next": "v13.2.1",
    "next-connect": "v1.0.0-next.3",
    "next-intl": "^2.11.0-beta.15",
    "picocolors": "1.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "12.2.0",
    "server-only": "0.0.1"
  },
  "devDependencies": {
    "@armit/eslint-config-bases": "workspace:^",
    "@emotion/babel-plugin": "11.10.6",
    "@emotion/react": "11.10.6",
    "@next/bundle-analyzer": "13.2.1",
    "@next/env": "13.2.1",
    "@playwright/test": "1.31.1",
    "@size-limit/file": "8.2.4",
    "@svgr/webpack": "6.5.1",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/forms": "0.5.3",
    "@tailwindcss/line-clamp": "0.4.2",
    "@tailwindcss/typography": "0.5.9",
    "@testing-library/dom": "9.0.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/multiparty": "0.0.33",
    "@types/node": "18.14.2",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@vitejs/plugin-react": "3.1.0",
    "@vitest/ui": "0.29.2",
    "autoprefixer": "10.4.13",
    "cross-env": "7.0.3",
    "es-check": "7.1.0",
    "eslint": "8.35.0",
    "eslint-config-next": "13.2.1",
    "happy-dom": "8.9.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.21",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-preset-env": "8.0.1",
    "prettier": "2.8.4",
    "rimraf": "4.1.2",
    "size-limit": "8.2.4",
    "symlink-dir": "5.1.1",
    "sync-directory": "6.0.2",
    "tailwindcss": "3.2.7",
    "typescript": "4.9.5",
    "vite": "4.1.4",
    "vite-plugin-svgr": "2.4.0",
    "vite-tsconfig-paths": "4.0.5",
    "vitest": "0.29.2"
  }
}
