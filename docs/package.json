{
  "name": "@armit/docs",
  "version": "0.0.17",
  "private": true,
  "homepage": "https://github.com/armitjs/armit",
  "repository": {
    "type": "git",
    "url": "https://github.com/armitjs/armit",
    "directory": "packages/docs"
  },
  "license": "MIT",
  "author": {
    "name": "Yingchun Tian",
    "url": "https://github.com/tianyingchun"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist/",
    "index.js",
    "README.md",
    "CHANGELOG.md",
    "package.json"
  ],
  "scripts": {
    "build": "docusaurus build",
    "clean": "rimraf --no-glob ./dist ./_release ./coverage ./tsconfig.tsbuildinfo",
    "clear": "docusaurus clear",
    "deploy": "docusaurus deploy",
    "docusaurus": "docusaurus",
    "lint": "eslint . --cache --cache-location ../.cache/eslint/docs.eslintcache",
    "fix-all-files": "eslint . --fix",
    "serve": "docusaurus start",
    "start": "docusaurus serve",
    "swizzle": "docusaurus swizzle",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "write-heading-ids": "docusaurus write-heading-ids",
    "write-translations": "docusaurus write-translations"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@docusaurus/core": "3.2.0",
    "@docusaurus/preset-classic": "3.2.0",
    "@emotion/react": "11.11.4",
    "@emotion/styled": "11.11.5",
    "@mdx-js/react": "3.0.1",
    "@mui/material": "5.15.14",
    "@sapphire/docusaurus-plugin-npm2yarn2pnpm": "2.0.2",
    "clsx": "2.1.0",
    "docusaurus-plugin-search-local": "2.0.1",
    "docusaurus-plugin-typedoc": "0.22.0",
    "mui-color-input": "2.0.3",
    "prism-react-renderer": "2.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typedoc": "0.25.12",
    "typedoc-plugin-markdown": "3.17.1",
    "typescript": "5.4.3"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.2.0",
    "@hyperse/eslint-config-hyperse": "^1.0.7",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/forms": "0.5.7",
    "@tailwindcss/line-clamp": "0.4.4",
    "@tailwindcss/typography": "0.5.12",
    "@tsconfig/docusaurus": "2.0.3",
    "autoprefixer": "10.4.19",
    "docusaurus-tailwindcss": "0.1.0",
    "eslint": "8.57.0",
    "postcss": "8.4.38",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-preset-env": "9.5.3",
    "tailwindcss": "3.4.3"
  },
  "engines": {
    "node": ">=16.14"
  },
  "publishConfig": {
    "access": "public"
  }
}
