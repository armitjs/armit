{
  "name": "@armit/docs",
  "version": "0.0.4",
  "private": true,
  "homepage": "https://github.com/armitjs/armit",
  "repository": {
    "type": "git",
    "url": "https://github.com/armitjs/armit",
    "directory": "packages/docs"
  },
  "license": "MIT",
  "author": {
    "name": "Yingchun Tian",
    "url": "https://github.com/tianyingchun"
  },
  "sideEffects": false,
  "types": "./dist/index.d.ts",
  "files": [
    "dist/",
    "index.js",
    "README.md",
    "CHANGELOG.md",
    "package.json"
  ],
  "scripts": {
    "build": "docusaurus build",
    "clean": "rimraf --no-glob ./dist ./coverage ./tsconfig.tsbuildinfo",
    "clear": "docusaurus clear",
    "deploy": "docusaurus deploy",
    "docusaurus": "docusaurus",
    "lint": "eslint . --ext .ts,.mts,.tsx,.js,.jsx,.cjs,.mjs --cache --cache-location ../.cache/eslint/docs.eslintcache",
    "serve": "docusaurus start",
    "start": "docusaurus serve",
    "swizzle": "docusaurus swizzle",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "write-heading-ids": "docusaurus write-heading-ids",
    "write-translations": "docusaurus write-translations"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@docusaurus/core": "2.3.0",
    "@docusaurus/preset-classic": "2.3.0",
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "@mdx-js/react": "2.2.1",
    "@mui/material": "5.11.7",
    "@sapphire/docusaurus-plugin-npm2yarn2pnpm": "1.1.4",
    "clsx": "1.2.1",
    "docusaurus-plugin-search-local": "0.9.0",
    "docusaurus-plugin-typedoc": "0.18.0",
    "mui-color-input": "1.0.4",
    "prism-react-renderer": "1.3.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typedoc": "0.23.24",
    "typedoc-plugin-markdown": "3.14.0",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "@armit/eslint-config-bases": "workspace:^",
    "@docusaurus/module-type-aliases": "2.3.0",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/forms": "0.5.3",
    "@tailwindcss/line-clamp": "0.4.2",
    "@tailwindcss/typography": "0.5.9",
    "@tsconfig/docusaurus": "1.0.6",
    "autoprefixer": "10.4.13",
    "docusaurus-tailwindcss": "0.1.0",
    "eslint": "8.33.0",
    "postcss": "8.4.21",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-preset-env": "8.0.1",
    "tailwindcss": "3.2.4"
  },
  "engines": {
    "node": ">=16.14"
  },
  "publishConfig": {
    "access": "public"
  }
}
